<div id="content-header">
  <img src="/images/show_finance.png" alt="" />
  <h1><%= t('app.views.finance.fees_index.fees') %></h1>
  <h3><%= t('app.views.finance.fees_index.manages_fees') %></h3>
  <div id="app-back-button">
    <%= link_to_function image_tag("/images/buttons/back.png",:border => 0), "history.back()" %>
  </div>
</div>

<div id="page-yield">

  <% unless flash[:notice].nil? %><p class="flash-msg"><%= flash[:notice] %></p><% end %>

  <div class="box">
    <div class="link-box">
      <div class="link-heading">
	<%= link_to t('app.views.finance.admin_reports_index.invoices'), 
                    :action => "request_invoices_report" %>
      </div>
      <div class="link-descr">
	<%= t('app.views.finance.admin_reports_index.invoices_desc') %>
      </div>
    </div>
  </div>
</div>
